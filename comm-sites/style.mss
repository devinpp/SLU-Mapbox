#commsitesnew [zoom>=9]{ 
[zoom=8] { point-transform:"scale(.01)";}
[zoom=9] { point-transform:"scale(.03)";}
[zoom=10] { point-transform:"scale(.04)";}
[zoom=11] { point-transform:"scale(.05)";}
[zoom=12] { point-transform:"scale(.07)";}
[zoom=13] { point-transform:"scale(.07)";}
[zoom=14] { point-transform:"scale(.09)";}
[zoom=15] { point-transform:"scale(.09)";}
[zoom=16] { point-transform:"scale(.09)";}  
point-file: url(icons/ComAroundSLU.svg);  
}

#commsitesnew [zoom>=10]{
text-allow-overlap: true;
text-wrap-width: 1;
text-wrap-character: '_';
text-name: [Label];
text-face-name: 'Times New Roman Regular';
text-size: 10;
text-halo-fill: fadeout(white, 30%);
text-halo-radius: 1.5;
text-placement-type: simple;
text-placements: "S";
text-vertical-alignment: bottom;
text-dy: 20;
}
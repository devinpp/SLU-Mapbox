




#stationsslu2014 [zoom>=9]{
[zoom=7] { point-transform:"scale(.02)";}
[zoom=8] { point-transform:"scale(.03)";}
[zoom=9] { point-transform:"scale(.05)";}
[zoom=10] { point-transform:"scale(.07)";}
[zoom=11] { point-transform:"scale(.10)";}
[zoom=12] { point-transform:"scale(.12)";}
[zoom=13] { point-transform:"scale(.18)";}
[zoom=14] { point-transform:"scale(.20)";}
[zoom=15] { point-transform:"scale(.25)";}
[zoom=16] { point-transform:"scale(.28)";}  
point-file: url(icons/g3813.svg);
point-allow-overlap: true;
}

#stationsslu2014 [zoom>=9]{
text-allow-overlap: true;
text-name: [aka];
text-face-name: 'Times New Roman Bold';
text-size: 12;
text-halo-fill: fadeout(white, 20%);
text-halo-radius: 2.5;
text-placement-type: simple;
text-placements: "N,S,E,W,NE,SE,NW,SW,14,12,10";
text-vertical-alignment: auto;
}